{
	"name": "UniSearch",
	"version": "1.0.0",
	"description": "Institutions Directory",
	"main": "index.ts",
	"author": "Dimitris Tzilopoulos - Nikos Tsompanidis",
	"license": "MIT",
	"private": true,
	"scripts": {
		"build": "tsc",
		"dev": "nodemon",
		"start": "node ./build/index.js",
		"seed": "ts-node --files ./src/utils/seeder/index.ts",
		"crawl_places": "ts-node --files ./src/utils/mapbox/crawler.ts",
		"watch": "mocha-typescript-watch",
		"test": "NODE_ENV=testing mocha --timeout 5000",
		"generate": "HYGEN_TMPLS=generators hygen new ",
		"seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
		"seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
		"typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.js"
	},
	"dependencies": {
		"apollo-server-express": "^2.16.1",
		"class-validator": "^0.12.2",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-pino-logger": "^5.0.0",
		"graphql": "^15.3.0",
		"graphql-depth-limit": "^1.1.0",
		"pg": "^8.3.0",
		"pino": "^6.5.0",
		"pino-pretty": "^4.1.0",
		"reflect-metadata": "^0.1.13",
		"type-graphql": "^1.0.0-rc.3",
		"typeorm": "^0.2.25",
		"typeorm-seeding": "^1.6.1",
		"typescript": "^3.9.7"
	},
	"devDependencies": {
		"@types/express": "^4.17.7",
		"@types/faker": "^4.1.12",
		"@types/node": "^14.0.27",
		"@types/pino": "^6.3.0",
		"@typescript-eslint/eslint-plugin": "^3.7.1",
		"@typescript-eslint/parser": "^3.7.1",
		"eslint": "^7.6.0",
		"eslint-config-airbnb-typescript": "^9.0.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-promise": "^4.2.1",
		"hygen": "^6.0.4",
		"nodemon": "^2.0.4",
		"prettier": "^2.0.5",
		"ts-node": "^8.10.2",
		"tsconfig-paths": "^3.9.0"
	}
}
